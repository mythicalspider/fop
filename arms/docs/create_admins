use systemhub;

replace into user 
    (user_id, name       , full_name) values
    (2      , 'jgurr'    , 'Jeremy Gurr')
  , (3      , 'cwojtyna' , 'Chris Wojtyna')
  , (4      , 'ablumberg', 'Aaron Blumberg')
  ;

replace into user_group_to_user 
    (user_group_id, user_id) values
    (1            , 2      )
  , (1            , 3      )
  , (1            , 4      )
  ;

